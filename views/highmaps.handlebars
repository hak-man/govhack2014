<div class="row-fluid">
    <div class="col-md-12">
        <div class="page-header"><h1>Highmaps Playbox</h1></div>
        <div id="highmaps"></div>
    </div>
</div>

<script type="text/javascript">
    $(function () {
        $.getJSON('data/world-data.json', function (data) {

            // Initiate the chart
            $('#highmaps').highcharts('Map', {
                title: {
                    text: null
                },
                exporting: {
                    enabled: false
                },
                credits: {
                    enabled: false
                },
                mapNavigation: {
                    enabled: true,
                    buttonOptions: {
                        verticalAlign: 'bottom'
                    }
                },

                colorAxis: {
                    min: 1,
                    max: 1000,
                    minColor: '#efedf5',
                    maxColor: '#756bb1',
                    type: 'logarithmic'
                },

                series : [{
                    data : data,
                    mapData: Highcharts.maps['custom/world'],
                    joinBy: ['iso-a2', 'code'],
                    name: 'Population density',
                    states: {
                        hover: {
                            color: '#BADA55'
                        }
                    },
                    tooltip: {
                        valueSuffix: '/kmÂ²'
                    }
                }]
            });
        });
    });
</script>
<script type="text/javascript" src="js/highmaps.js"></script>
<script type="text/javascript" src="http://code.highcharts.com/mapdata/custom/world.js"></script>